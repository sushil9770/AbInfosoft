<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


   

<constant name="struts.convention.result.path" value="/"></constant>
<package name="abc" extends="struts-default">



		
		

<interceptors>
                <interceptor class="com.interceptor.LoginInterceptor" name="loginInterceptor">
                </interceptor>
                <interceptor-stack name="loginStack">
                        <interceptor-ref name="loginInterceptor" />
                        <interceptor-ref name="defaultStack" />
                </interceptor-stack>
                
               
        </interceptors>
        
        
      

<global-results>
<result name="login">index.jsp</result>

<result name="invalid.token">utility_404_example2.jsp</result>
	

</global-results>
 	
 
 
 

<action name="login" method="login" class="com.rpts.action.LoginAction">

<result name="success"  >select_org.jsp</result>
<result name="error" >index.jsp</result>
<result name="input">index.jsp</result>
<result name="update_pass">update_pass.jsp</result>
</action>




<!--  -->




</package>


	
</struts>    
